{
  "name": "touch",
  "displayName": "Touch",
  "description": "touch files like touch command ",
  "icon": "images/Logo.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "homepage": "https://github.com/YoRolling/vscode-touch",
  "author": {
    "name": "YoRolling",
    "url": "https://github.com/YoRolling"
  },
  "repository": {
    "url": "https://github.com/YoRolling/vscode-touch",
    "directory": ""
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "utils",
    "files",
    "new file",
    "touch-like"
  ],
  "publisher": "YoRolling",
  "activationEvents": [
    "onCommand:touch.newFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "touch.newFile",
          "group": "2_workspace"
        }
      ]
    },
    "commands": [
      {
        "command": "touch.newFile",
        "title": "Creating File",
        "category": "Touch"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/braces": "^3.0.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "braces": "^3.0.2"
  }
}